{
    "name": "Lidarr",
    "id": "lidarr",
    "packaging_format": 1,
    "description": {
        "en": "A music collection manager for Usenet and BitTorrent users.",
        "fr": "Un gestionnaire de discothÃ¨que pour utilisateurs de Usenet et BitTorrent."
    },
    "version": "0.8.1.2135~ynh1",
    "url": "https://lidarr.audio",
    "upstream": {
        "license": "GPL-3.0-only",
        "website": "https://lidarr.audio",
        "admindoc": "https://wiki.servarr.com/Lidarr",
        "code": "https://github.com/Lidarr/Lidarr"
    },
    "license": "MIT",
    "maintainer": {
        "name": "tituspijean",
        "email": "tituspijean@outlook.com"
    },
    "requirements": {
        "yunohost": ">= 4.1.3"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php7.3-fpm",
        "mysql"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/lidarr",
                "default": "/lidarr"
            },
            {
                "name": "admin",
                "type": "user",
                "example": "johndoe"
            }
        ]
    }
}
